{
    "name": "kenny1911/bolt-psysh",
    "description": "PsySH for BoltCMS",
    "type": "bolt-extension",
    "keywords": ["bolt", "bolt-cms", "boltcms", "console", "debug", "psysh", "repl"],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0.0",
        "ext-json": "*",
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Myzyka",
            "email": "o-muzyka@mail.ru",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Kenny1911\\BoltPsysh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Kenny1911\\BoltPsysh\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Kenny1911\\BoltPsysh\\BoltPsyshExtension"
    }
}
